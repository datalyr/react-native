{
  "name": "@datalyr/react-native",
  "version": "1.0.2",
  "description": "Datalyr SDK for React Native & Expo - Server-side attribution tracking",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib/",
    "src/",
    "*.md",
    "examples/"
  ],
  "keywords": [
    "react-native",
    "expo",
    "skadnetwork",
    "attribution",
    "tracking",
    "mobile",
    "datalyr",
    "appsflyer",
    "adjust",
    "mixpanel",
    "automatic-events",
    "session-tracking",
    "utm-tracking",
    "facebook-attribution",
    "tiktok-attribution",
    "google-attribution",
    "ios-attribution",
    "conversion-tracking",
    "revenue-optimization"
  ],
  "author": "Datalyr",
  "license": "MIT",
  "homepage": "https://datalyr.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/datalyr/react-native.git"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "react-native-device-info": ">=12.0.0"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "react-native-get-random-values": "^1.11.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^30.0.0",
    "typescript": "^5.7.0"
  },
  "react-native": "src/index.ts",
  "exports": {
    ".": {
      "react-native": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./expo": {
      "react-native": "./src/expo.ts",
      "default": "./lib/expo.js"
    }
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    },
    "expo-device": {
      "optional": true
    },
    "expo-application": {
      "optional": true
    },
    "expo-network": {
      "optional": true
    },
    "expo-tracking-transparency": {
      "optional": true
    },
    "react-native-device-info": {
      "optional": true
    }
  }
}
